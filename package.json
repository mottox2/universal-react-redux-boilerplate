{
  "name": "universal-react-redux-boilerplate",
  "version": "2.0.0",
  "description": "Step by step creation of a Universal React + Redux Boilerplate",
  "main": "index.js",
  "scripts": {
    "dev": "node --trace-warnings ./bin/server",
    "dev-debug": "node --inspect --debug-brk --trace-warnings ./bin/server",
    "start": "cross-env NODE_ENV=production node ./bin/server",
    "build": "npm run build:server && npm run build:client",
    "build:server": "cross-env NODE_ENV=production babel src --out-dir lib --copy-files",
    "build:client": "cross-env NODE_ENV=production webpack --progress",
    "clean": "rimraf dist lib",
    "check": "npm run lint && npm run test && npm run flow",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint --cache .",
    "lint:css": "stylelint src/**/*.scss --syntax scss",
    "test": "cross-env NODE_ENV=test mocha mocha.config.js",
    "flow": "flow",
    "flow:stop": "flow stop"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CrocoDillon/universal-react-redux-boilerplate.git"
  },
  "keywords": [
    "isomorphic",
    "universal",
    "react",
    "redux",
    "boilerplate"
  ],
  "author": "Dillon de Voor (http://crocodillon.com)",
  "license": "CC0-1.0",
  "dependencies": {
    "@babel/preset-env": "^7.0.0-beta.49",
    "@babel/preset-flow": "^7.0.0-beta.49",
    "@types/react": "^16.3.16",
    "@types/react-dom": "^16.0.6",
    "isomorphic-fetch": "^2.2.1",
    "koa": "^2.3.0",
    "koa-static": "^4.0.0",
    "prop-types": "^15.5.10",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-helmet": "^5.1.3",
    "react-redux": "^5.0.5",
    "react-router-config": "^1.0.0-beta.3",
    "react-router-dom": "^4.1.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "webpack-isomorphic-tools": "^3.0.3"
  },
  "devDependencies": {
    "@babel/cli": "latest",
    "@babel/core": "^7.0.0-beta.49",
    "@babel/preset-es2015": "latest",
    "@babel/preset-react": "latest",
    "@babel/preset-stage-0": "latest",
    "@babel/preset-typescript": "^7.0.0-beta.49",
    "@babel/register": "latest",
    "autoprefixer": "^7.1.2",
    "babel-eslint": "latest",
    "babel-loader": "^8.0.0-beta",
    "babel-plugin-add-module-exports": "latest",
    "chai": "^4.0.2",
    "chai-as-promised": "^7.1.1",
    "chai-enzyme": "^0.8.0",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "css-modules-require-hook": "^4.0.6",
    "enzyme": "^2.9.1",
    "eslint": "^4.1.1",
    "eslint-plugin-flowtype": "^2.34.1",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "flow-bin": "^0.49.1",
    "glob": "^7.1.2",
    "jsdom": "^11.1.0",
    "mocha": "^3.4.2",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "react-hot-loader": "^4.3.0",
    "react-test-renderer": "^15.6.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "sinon": "^2.3.6",
    "sinon-chai": "^2.11.0",
    "style-loader": "^0.18.2",
    "stylelint": "^7.12.0",
    "typescript": "^2.9.1",
    "webpack": "^3.1.0",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-hot-middleware": "^2.18.2"
  }
}
